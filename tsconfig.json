{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "downlevelIteration": true,
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "src/components/**.tsx", "src/lib/auth.js", "src/components/cards/Testing.js", "src/__tests__/components/AccountWrapper.test.tsx", "src/__tests__/components/AccountWrapper.test.tsx", "src/__tests__/utils/SessionStorage.test.js"],
  "exclude": ["node_modules"]
}
